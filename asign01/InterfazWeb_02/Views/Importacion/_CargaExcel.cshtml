@using (Html.BeginForm("SubirExcel", "Importacion", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <label>Cargar Excel:</label>
    <br />
    <input type="file" name="file" accept=".xlsx, .xls"/> 
    <button type="submit">Subir archivo</button>
}
<script src="~/JS/_CargaArchivo.js"></script>