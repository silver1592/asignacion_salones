<div class="row">
    <div class="col-md-12">
        <h3>Ejecuta Asignacion</h3>
    </div>
</div>
<div class="contenedor">
    <div class="row">
        <div class="col-md-6 text-center">
            <label>Hora de inicio</label>&nbsp;
            <input type="number" name="hora_ini" value="7" />

        </div>
        <div class="col-md-6 text-center">
            <label>Hora de Limite</label>&nbsp;
            <input type="number" name="hora_fin" value="22" />
        </div>
    </div>
    <br />
    <table>
        <tr>
            <td valign="top" class="separador">
                <input type="checkbox" name="emp" value="e" /><label><u>Empalmes</u></label>
            </td>

            <td valign="top" class="separador">
                <input type="checkbox" name="pre" value="p" /><label><u>Preasignacion</u></label>
                <br />
                <label>Otros semestres</label><input type="checkbox" name="pre_otros_sem" value="f" />
                <br />
            </td>
            <td valign="top">
                <input type="checkbox" name="asig" value="a" /><label><u>Algoritmo</u></label>
                <br />
                <label>Individuos</label>&nbsp;<input type="number" name="alg_individuos" value="5" />
                <br />
                <label>Generaciones</label>&nbsp;<input type="number" name="alg_generaciones" value="50" />
            </td>
        </tr>
    </table>
    <div class="row">
        <div class="col-md-12 text-center">
            <button id="EjecutaOperaciones">Ejecutar</button>
        </div>
    </div>
</div>
<div class="direccion">
    <p id="Ejecuta">@Url.Action("EjecutaOperaciones", "Ejecuta",null, Request.Url.Scheme)</p>
</div>
<script src="~/JS/_Ejecuta.js"></script>